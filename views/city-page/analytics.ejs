<amp-analytics type="googleanalytics">
        <script type="application/json">
        {
          "vars": {
                   "account": "UA"
               },
               
          "triggers": {
               "trackPageview": {
                  "on": "visible",
                  "request": "pageview"
                },
                "hamburgerClick": {
                    "on": "click",
                    "selector": ".sidebar__nav",
                    "request": "event",
                    "vars": {
                      "eventCategory": "List Page",
                      "eventAction": "click",
                      "eventLabel": "CLICK_HAMBURGER"
                    }
               }          
            }
        }
        </script>
    </amp-analytics>